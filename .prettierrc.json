{
    "parser": "typescript",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "html",
          "tabWidth": 4
        }
      },
      {
        "files": "*.css",
        "options": {
          "parser": "css",
          "tabWidth": 4
        }
      }
    ]
  }